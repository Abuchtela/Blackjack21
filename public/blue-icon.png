<svg width="512" height="512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="iconGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0052FF"/>
      <stop offset="100%" style="stop-color:#0041CC"/>
    </linearGradient>
  </defs>
  
  <!-- Rounded square background -->
  <rect width="512" height="512" rx="100" fill="url(#iconGradient)"/>
  
  <!-- White card symbols for blackjack theme -->
  <g transform="translate(256, 256)">
    <!-- Ace of Spades icon -->
    <path d="M -80 -60 L -50 -100 L -20 -60 C -20 -40 -35 -30 -50 -30 C -65 -30 -80 -40 -80 -60 Z" fill="white"/>
    
    <!-- King symbol -->
    <circle cx="0" cy="-70" r="35" fill="white"/>
    <rect x="-15" y="-45" width="30" height="60" fill="white"/>
    <polygon points="-15,-50 15,-50 0,-75" fill="#0052FF"/>
    
    <!-- Heart -->
    <path d="M 40 -60 C 40 -75 52 -85 65 -85 C 78 -85 85 -75 85 -65 C 85 -45 65 -30 65 -30 C 65 -30 45 -45 45 -65 C 45 -75 52 -85 65 -85" fill="white"/>
    <path d="M 65 -85 C 78 -85 90 -75 90 -65 C 90 -45 65 -25 65 -25 C 65 -25 40 -45 40 -65 C 40 -75 52 -85 65 -85" fill="white"/>
    
    <!-- Chips/tokens at bottom -->
    <ellipse cx="-50" cy="50" rx="30" ry="8" fill="white" opacity="0.9"/>
    <ellipse cx="0" cy="60" rx="30" ry="8" fill="white" opacity="0.9"/>
    <ellipse cx="50" cy="50" rx="30" ry="8" fill="white" opacity="0.9"/>
    
    <!-- Game text -->
    <text x="0" y="95" font-family="Arial, sans-serif" font-size="32" font-weight="bold" 
          text-anchor="middle" fill="white">21</text>
  </g>
</svg>
